#!/bin/bash

set -e

source "$STATE"

if [ -n "$TEST_AND_PUBLISH" ] ; then
    rm -f /tmp/gce_setup_done
    cd $SRCDIR/test
    ./gce.sh setup > /tmp/gce_setup_output 2>&1
    eval $(./gce.sh hosts) >> /tmp/gce_setup_output 2>&1
    ./setup.sh >> /tmp/gce_setup_output 2>&1
    echo "done" > /tmp/gce_setup_done
fi
