{
  "description": "Weave Net plugin for Docker",
  "documentation": "https://weave.works",
  "entrypoint": [
    "/home/weave/launch.sh"
  ],
  "interface": {
    "socket": "weave.sock",
    "types": [
      "docker.networkdriver/1.0"
    ]
  },
  "linux": {
    "capabilities": [
      "CAP_SYS_ADMIN",
      "CAP_NET_ADMIN",
      "CAP_SYS_MODULE"
    ]
  },
  "mounts": [
    {
      "destination": "/host/proc/",
      "type": "bind",
      "source": "/proc/",
      "options": ["rbind", "rw"]
    },
    {
      "destination": "/var/run/docker.sock",
      "source": "/var/run/docker.sock",
      "type": "bind",
      "options": ["rbind"]
    },
    {
      "destination": "/host/var/lib/",
      "source": "/var/lib/",
      "type": "bind",
      "options": ["rbind"]
    },
    {
      "destination": "/host/etc/",
      "source": "/etc/",
      "type": "bind",
      "options": ["rbind"]
    },
    {
      "destination": "/lib/modules/",
      "source": "/lib/modules/",
      "type": "bind",
      "options": ["rbind"]
    }
  ],
  "env": [
    {
      "description": "Log level",
      "name": "LOG_LEVEL",
      "settable": [
          "value"
      ],
      "value": ""
    },
    {
      "description": "Extra args to `weaver` and `plugin`",
      "name": "EXTRA_ARGS",
      "settable": [
          "value"
      ],
      "value": ""
    },
    {
      "description": "Encryption password",
      "name": "WEAVE_PASSWORD",
      "settable": [
          "value"
      ],
      "value": ""
    },
    {
      "description": "MTU",
      "name": "WEAVE_MTU",
      "settable": [
          "value"
      ],
      "value": ""
    },
    {
      "description": "Enable multicast for all Weave networks",
      "name": "WEAVE_MULTICAST",
      "settable": [
          "value"
      ],
      "value": ""
    },
    {
      "description": "The range of IP addresses used by Weave Net",
      "name": "IPALLOC_RANGE",
      "settable": [
          "value"
      ],
      "value": ""
    }
  ],
  "network": {
    "type": "host"
  },
  "workdir": ""
}
